<DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
</head>
	<body>
		<script>
		function Summator(){
			this.sum = function(a, b){
				return a + b;
			};

			this.run = function(){
				var a = +prompt('a?', 0);
				var b = +prompt("b?", 0);
				alert('sum = ' + this.sum(a, b));
			};
		}

		new Summator().run();
		</script>
	</body>
</html>
